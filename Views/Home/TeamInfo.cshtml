@{
}
@model List<Team>


    <table class="table table-bordered table-striped">
        <thead>
            <tr>
                <th>Team ID</th>
                <th>Team Name</th>

            </tr>
        </thead>
        <tbody>
            @foreach (var a in Model)
            {
                <tr>
                    <td>@a.TeamID</td>
                    <td>@a.TeamName</td>
                </tr>
            }
        </tbody>
    </table>
